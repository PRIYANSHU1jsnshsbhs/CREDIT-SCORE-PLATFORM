{
  "builds": [
    {
      "src": "react-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "react-frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "react-frontend/dist/$1"
    }
  ],
  "buildCommand": "cd react-frontend && npm run build",
  "outputDirectory": "react-frontend/dist",
  "installCommand": "cd react-frontend && npm install"
}
